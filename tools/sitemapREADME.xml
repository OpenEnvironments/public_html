HEADER SECTION INCLUDES MAIN PAGES
THEN THERES A SQL STATEMENT TO GENERATE THE DATA DRIVEN REFERENCES
LAST THERES A CLOSING urlset tag



<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
<url>
  <loc>http://www.openenvironments.com/</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/index.php</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/register.php</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/forgot.php</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/events.php?mode=list</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/links.php?mode=list&amp;cat=1</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/links.php?mode=list&amp;cat=4</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/links.php?mode=list&amp;cat=3</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/links.php?mode=list&amp;cat=2</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/documents.php?mode=list</loc>
  <changefreq>daily</changefreq>
</url>
<url>
  <loc>http://www.openenvironments.com/contributors.php</loc>
  <changefreq>daily</changefreq>
</url>


<!-- below are the listing of events, links, documents and contributors -->


SELECT concat("<url><loc>http://www.openenvironments.com/events.php?mode=view&ID=",Event_ID,"</loc></url>") URL FROM events 
union
SELECT concat("<url><loc>http://www.openenvironments.com/links.php?mode=view&ID=",Link_ID,"</loc></url>") URL FROM links
union
SELECT concat("<url><loc>http://www.openenvironments.com/contributors.php?mode=view&ID=",Contributor_ID,"</loc></url>") URL FROM contributors
union
SELECT concat("<url><loc>http://www.openenvironments.com/documents.php?mode=view&ID=",Doc_ID,"</loc></url>") URL FROM documents


</urlset>